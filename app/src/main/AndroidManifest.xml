<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.biyan.ubama">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <service
            android:name=".messaging.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>

        <service
            android:name=".messaging.MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>

        <activity
            android:name=".welcome.SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.app.default_searchable"
                android:value=".HasilPencarianActivity" />
        </activity>
        <activity
            android:name=".welcome.WelcomeActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".welcome.LoginActivity"
            android:label="Masuk"
            android:parentActivityName=".welcome.WelcomeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".welcome.WelcomeActivity" />
        </activity>
        <activity
            android:name=".welcome.RegisterActivity"
            android:label="Daftar"
            android:parentActivityName=".welcome.WelcomeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".welcome.WelcomeActivity" />
        </activity>
        <activity
            android:name=".HasilPencarianActivity"
            android:label="Hasil Pencarian">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />

            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BarangJasaActivity"
            android:label="Detail Produk"
            android:launchMode="singleTask">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".keranjang.KeranjangActivity"
            android:label="Keranjang">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".pesanan.PesananActivity"
            android:label="Pesanan"
            android:launchMode="singleTask">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".PemesananActivity"
            android:label="Pemesanan Produk">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".BarangJasaActivity" />
        </activity>
        <activity
            android:name=".beranda.HasilFakultasActivity"
            android:label="Fakultas">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".beranda.SubkategoriActivity"
            android:label="Kategori"
            android:launchMode="singleTask">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".beranda.HasilSubkategoriActivity"
            android:label="Subkategori">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".beranda.SubkategoriActivity" />
        </activity>
        <activity
            android:name=".pesanan.DetailPesananActivity"
            android:label="Detail Pesanan">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".pesanan.PesananActivity" />
        </activity>
        <activity
            android:name=".toko.TokoUserActivity"
            android:label="Toko">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".toko.TokoCreateActivity"
            android:label="Toko">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".TokoActivity" />
        <activity android:name=".komentar.KomentarActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".BarangJasaActivity" />
        </activity>
        <activity android:name=".tanyajawab.TanyaJawabActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".BarangJasaActivity" />
        </activity>
        <activity android:name=".tanyajawab.KirimPertanyaanActivity" />
        <activity android:name=".toko.TokoJualProdukActivity" />
        <activity android:name=".toko.TokoPengaturanActivity" />
        <activity
            android:name=".toko.TokoProdukActivity"
            android:label="@string/title_activity_toko_produk"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".toko.TokoPesananActivity"
            android:label="@string/title_activity_toko_pesanan"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".toko.TokoTanyaJawabActivity"
            android:label="@string/title_activity_toko_tanya_jawab"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>